extends layout

block content
	h1 lib.nekobato.net
	ul.log
		for v, k in data
			li
				span #{v.time} 
				a(href="#{site}article/#{v.id}") #{v.title}
	article.article(data-id= article.id)
		h2
			a(href=site + "article/" + article.id) #{article.title}
		p.time #{article.time}
	footer
		p lib.nekobato.net
